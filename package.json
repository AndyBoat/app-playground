{
  "name": "app-playground",
  "private": true,
  "scripts": {
    "dev:next": "next dev --turbo",
    "dev:tailwind": "npx tailwindcss -i styles/globals.css -o styles/dist.css --postcss=postcss.config.js --watch",
    "dev": "turbo dev:turbo dev:tailwind",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "npx tailwindcss -i styles/globals.css -o styles/dist.css --postcss=postcss.config.js"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "clsx": "1.2.1",
    "next": "npm:@vercel/next-22-10@12.3.2-rc.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.0.0-beta.2",
    "use-count-up": "3.0.1"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.7",
    "eslint": "8.19.0",
    "eslint-config-next": "^12.3.2-canary.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6",
    "turbo": "1.6.1",
    "typescript": "4.7.4"
  }
}
